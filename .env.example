# Copy to .env and replace with your real values
PUBLIC_HOST=brain.example.com
ADMIN_HOST=admin.brain.example.com
SPACE_DOMAIN_SUFFIX=spaces.brain.example.com
VIRTUAL_HOST=brain.example.com,*.brain.example.com
LETSENCRYPT_HOST=brain.example.com,*.brain.example.com
GATEKEEPER_PORT=80

SB_WRITER_USER=admin
SB_WRITER_PASSWORD=change-me

HOST_ROOT_DIR=/home/you/docker/ng-brain

# Remote content repo (required)
CONTENT_REMOTE=git@github.com:you/your-notes.git
CONTENT_BRANCH=main
# Optional (base64-encoded SSH material if not mounting ~/.ssh)
# CONTENT_REMOTE_SSH_KEY=
# CONTENT_REMOTE_SSH_KNOWN_HOSTS=

DEFAULT_EMAIL=you@example.com
CF_Token=apiTokenFromCFDashboard